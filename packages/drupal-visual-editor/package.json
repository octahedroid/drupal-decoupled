{
  "name": "drupal-visual-editor",
  "version": "0.2.0",
  "description": "Provide delightful editorial experiences for content editors using Drupal regardless of your FE implementation.",
  "type": "module",
  "keywords": [
    "drupal",
    "dxp",
    "composable",
    "decoupled",
    "headless",
    "visual-editor",
    "visual",
    "editor",
    "studio",
    "builder"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bundlesize": [
    {
      "path": "./dist/index.js"
    }
  ],
  "scripts": {
    "prepublish": "yarn build",
    "ts-types": " tsc --emitDeclarationOnly --outDir dist",
    "build": "rimraf dist && node ./esbuild.js && yarn ts-types"
  },
  "author": "Octahedroid <opensource@octahedroid.com>",
  "repository": {
    "url": "git+https://github.com/octahedroid/drupal-decoupled.git#main"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.3",
    "esbuild": "^0.17.18",
    "rimraf": "^5.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {}
}
