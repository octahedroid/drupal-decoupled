{
  "name": "drupal-decoupled",
  "version": "0.2.2",
  "description": "Drupal utils for Decoupled integrations",
  "type": "module",
  "keywords": [
    "graphql",
    "drupal",
    "decoupled",
    "remix",
    "next.js"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "ts-types": "tsc --emitDeclarationOnly --outDir dist",
    "build": "node ./esbuild.js && pnpm ts-types"
  },
  "author": "Octahedroid <opensource@octahedroid.com>",
  "repository": {
    "url": "https://github.com/octahedroid/drupal-decoupled/tree/main/packages/drupal-decoupled"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./remix": {
      "types": "./dist/remix/index.d.ts",
      "import": "./dist/remix/index.js"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "catalog:",
    "esbuild": "catalog:",
    "typescript": "catalog:"
  }
}
