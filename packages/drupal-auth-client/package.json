{
  "name": "drupal-graphql-client",
  "version": "0.1.4",
  "description": "Drupal client for GraphQL",
  "keywords": [
    "graphql",
    "drupal"
  ],
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "bundlesize": [
    {
      "path": "./dist/index.js"
    }
  ],
  "scripts": {
    "prepublish": "yarn build",
    "build": "rm -rf dist && tsc -d"
  },
  "author": "Octahedroid <opensource@octahedroid.com>",
  "repository": {
    "url": "https://github.com/octahedroid/drupal-graphql-client"
  },
  "license": "MIT",
  "resolutions": {
    "graphql": "14 - 16"
  },
  "peerDependencies": {
    "graphql": "14 - 16"
  },
  "devDependencies": {
    "@types/node": "^18.11.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.1.1",
    "cross-fetch": "^3.1.5",
    "form-data": "^4.0.0",
    "graphql-request": "^5.0.0"
  }
}
