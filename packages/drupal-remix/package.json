{
  "name": "drupal-remix",
  "version": "0.1.3",
  "description": "Drupal utils for remix.run",
  "type": "module",
  "keywords": [
    "graphql",
    "drupal",
    "metatags",
    "seo",
    "remix",
    "remix.run",
    "remixjs"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bundlesize": [
    {
      "path": "./dist/index.js"
    }
  ],
  "scripts": {
    "prepublish": "yarn build",
    "ts-types": " tsc --emitDeclarationOnly --outDir dist",
    "build": "rimraf dist && node ./esbuild.js && yarn ts-types"
  },
  "author": "Octahedroid <opensource@octahedroid.com>",
  "repository": {
    "url": "https://github.com/octahedroid/drupal-remix/tree/main/packages/drupal-remix"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.3",
    "typescript": "^4.8.4",
    "esbuild": "^0.17.18",
    "rimraf": "^5.0.0"
  },
  "dependencies": {}
}
